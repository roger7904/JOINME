<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        android:id="@+id/nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navGraph="@navigation/mobile_navigation" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".MapsActivity"
            tools:layout_editor_absoluteX="47dp"
            tools:layout_editor_absoluteY="-57dp" />

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="462dp"
            android:layout_height="162dp"
            android:layout_marginTop="56dp"
            android:background="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/storebtn"
            android:layout_width="45dp"
            android:layout_height="74dp"
            android:layout_marginTop="124dp"
            android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp"
            android:background="@drawable/discount"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/ballbtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/ktvbtn"
            android:layout_width="45dp"
            android:layout_height="74dp"
            android:layout_marginStart="36dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="124dp"
            android:background="@drawable/ktv"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/informationbtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/informationbtn"
            android:layout_width="45dp"
            android:layout_height="74dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="124dp"
            android:background="@drawable/timelimit"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/ballbtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/ballbtn"
            android:layout_width="45dp"
            android:layout_height="74dp"
            android:layout_marginTop="124dp"
            android:layout_marginEnd="56dp"
            android:layout_marginRight="56dp"
            android:background="@drawable/ball"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/guideline"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/otherbtn"
            android:layout_width="45dp"
            android:layout_height="74dp"
            android:layout_marginStart="112dp"
            android:layout_marginLeft="112dp"
            android:layout_marginTop="124dp"
            android:background="@drawable/others"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/informationbtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/settingBtn"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginTop="652dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/setting"
            app:layout_constraintEnd_toStartOf="@+id/messageBtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/messageBtn"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginTop="652dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:background="@drawable/message"
            app:layout_constraintEnd_toStartOf="@+id/joBtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/joBtn"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="164dp"
            android:layout_marginLeft="164dp"
            android:layout_marginTop="632dp"
            android:background="@drawable/jo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/noticeBtn"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="28dp"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="652dp"
            android:background="@drawable/notice"
            app:layout_constraintStart_toEndOf="@+id/joBtn"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/collectBtn"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="652dp"
            android:background="@drawable/collection"
            app:layout_constraintStart_toEndOf="@+id/noticeBtn"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="207dp" />

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appBarLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="?attr/colorPrimary"
                app:popupTheme="@style/AppTheme.PopupOverlay" />

        </com.google.android.material.appbar.AppBarLayout>

        <EditText
            android:id="@+id/editsuffer"
            android:layout_width="202dp"
            android:layout_height="35dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="108dp"
            android:layout_marginRight="108dp"
            android:background="@drawable/shape_textview"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button16"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:layout_marginTop="22dp"
            android:layout_marginBottom="616dp"
            android:background="@drawable/p5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editsuffer"
            app:layout_constraintTop_toBottomOf="@+id/appBarLayout"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <!--    側選單未完成    -->

</FrameLayout>
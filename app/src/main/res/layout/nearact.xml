<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="408dp"
        android:layout_height="59dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <include layout="@layout/bottom_bar" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="401dp"
        android:layout_height="112dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text=" " />

        <Button
            android:id="@+id/likebtn"
            android:layout_width="15dp"
            android:layout_height="51dp"
            android:layout_weight="1"
            android:background="@drawable/p12" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text=" " />

        <Button
            android:id="@+id/applycounts"
            android:layout_width="7dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/p11" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text=" " />

        <Button
            android:id="@+id/talkbtn"
            android:layout_width="13dp"
            android:layout_height="51dp"
            android:layout_weight="1"
            android:background="@drawable/p10" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="  " />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="392dp"
        android:layout_height="149dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="212dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/head" />

        <TextView
            android:id="@+id/contenct"
            android:layout_width="239dp"
            android:layout_height="172dp"
            android:layout_weight="1"
            android:text="@string/actcontenct"
            android:textSize="24sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="411dp"
        android:layout_height="105dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <Button
            android:id="@+id/ballbtn"
            android:layout_width="66dp"
            android:layout_height="43dp"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="454dp"
            android:text="@string/ballsort" />

        <Button
            android:id="@+id/eatbtn"
            android:layout_width="66dp"
            android:layout_height="43dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="454dp"
            android:text="@string/eating" />

        <Button
            android:id="@+id/viewbtn"
            android:layout_width="66dp"
            android:layout_height="43dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="454dp"
            android:text="@string/information" />

        <Button
            android:id="@+id/tripbtn"
            android:layout_width="66dp"
            android:layout_height="43dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="454dp"
            android:text="@string/travel" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="413dp"
        android:layout_height="55dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <Button
            android:id="@+id/activitybtn"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="@drawable/selectactbtn"
            android:text="@string/activity"
            android:textSize="16dp" />

        <Button
            android:id="@+id/joinbtn"
            android:layout_width="82dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="@drawable/selectactbtn"
            android:text="@string/join"
            android:textSize="16dp" />

        <Button
            android:id="@+id/holdonbtn"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/selectactbtn"
            android:text="@string/holdon"
            android:textSize="16dp" />

        <Button
            android:id="@+id/nearactbtn"
            android:layout_width="69dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/selectactbtn"
            android:text="@string/nearact"
            android:textSize="16dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="453dp"
        android:layout_height="183dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7">

        <TextView
            android:id="@+id/textView"
            android:layout_width="213dp"
            android:layout_height="45dp"
            android:layout_marginStart="91dp"
            android:layout_marginLeft="91dp"
            android:layout_marginTop="82dp"
            android:layout_marginBottom="18dp"
            android:background="@drawable/outside" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="104dp"
            android:layout_height="127dp">

            <Button
                android:id="@+id/button16"
                android:layout_width="45dp"
                android:layout_height="48dp"
                android:layout_marginTop="82dp"
                android:layout_marginEnd="59dp"
                android:layout_marginRight="59dp"
                android:background="@drawable/p5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="411dp"
        android:layout_height="66dp"
        android:layout_marginTop="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>